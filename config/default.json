{
  "env": "development",
  "port": 3000,
  "swagger": {
    "enabled": true,
    "endpoint": "docs"
  },
  "rateLimit": {
    "enabled": true,
    "max": 100,
    "timeWindow": "1m"
  },
  "cors": {
    "origin": "*",
    "methods": "*",
    "allowedHeaders": "*"
  },
  "validator": {
    "forbidUnknown": true
  },
  "logging": {
    "timestampFormat": "YYYY-MM-DD HH:mm:ss.SSS"
  }
}
