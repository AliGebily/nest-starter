{
  "env": "development",
  "port": 3000,
  "swagger": {
    "enabled": true,
    "endpoint": "docs",
    "title": "nest-starter",
    "description": "A starter template for NestJS with support for swagger, config, logging, validation, exception handling, docker, testing and CI.",
    "contactEmail": "contact@marknjunge.com"
  },
  "rateLimit": {
    "enabled": true,
    "max": 100,
    "timeWindow": "1m"
  },
  "cors": {
    "origin": "*",
    "methods": "*",
    "allowedHeaders": "*"
  },
  "validator": {
    "forbidUnknown": true
  },
  "logging": {
    "timestampFormat": "YYYY-MM-DD HH:mm:ss.SSS"
  }
}
